package com.mycompany.condicionales_sharick;

/**
 *
 * @author Estudiantes
 */
public class Condicionales_sharick {

    public static void main(String[] args) {
        String canciones = "rock";
        
    if (canciones == "Jazz"){
        System.out.println("relajarse");
        System.out.println("tomar cafe");
        System.out.println("dormir");
    } else if (canciones == "rock"){
        System.out.println("agirtar cabeza");
        System.out.println("subir volumen");
        System.out.println("tener dolor de cabeza");
    }else{
        System.out.println("es bachata");
    }
    }
}

